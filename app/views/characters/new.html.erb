<%= form_with(model: @character, local: true) do |form| %>
    <div>
        <%= form.label :name%>
        <%= form.text_field :name%>
    </div>
    <div>
        <%=form.label :description %>
        <%=form.rich_text_area :description %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>